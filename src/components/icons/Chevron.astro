---
interface Props{
    width: number,
    height: number,
    transformation: string,
}


const { width, height, transformation } = Astro.props;
---

<svg xmlns="http://www.w3.org/2000/svg" width={width}  height={height} style={transformation} viewBox="0 0 24 24"  fill="none"  stroke="currentColor"  stroke-width="2"  stroke-linecap="round"  stroke-linejoin="round"  class="icon icon-tabler icons-tabler-outline icon-tabler-chevron-left" ><path stroke="none" d="M0 0h24v24H0z" fill="none" /><path d="M15 6l-6 6l6 6" /></svg>

<style>
    svg{
        stroke: var(--color-link);
    }
</style>